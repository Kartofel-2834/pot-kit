{
    "variables": {
        "pot-spacer": "1rem",
        "pot-unit": "calc(var(--pot-spacer) / 2)",
        "pot-spacer-0-400": "calc(var(--pot-spacer) * 0.4)",
        "pot-spacer-0-600": "calc(var(--pot-spacer) * 0.6)",
        "pot-spacer-0-800": "calc(var(--pot-spacer) * 0.8)",
        "pot-spacer-1-200": "calc(var(--pot-spacer) * 1.2)",
        "pot-spacer-1-400": "calc(var(--pot-spacer) * 1.4)",
        "pot-spacer-1-600": "calc(var(--pot-spacer) * 1.6)",
        "pot-spacer-1-800": "calc(var(--pot-spacer) * 1.8)",
        "pot-spacer-2": "calc(var(--pot-spacer) * 2)",
        "pot-spacer-2-400": "calc(var(--pot-spacer) * 2.4)",
        "pot-spacer-2-800": "calc(var(--pot-spacer) * 2.8)",
        "pot-spacer-3": "calc(var(--pot-spacer) * 3)",
        "pot-spacer-3-200": "calc(var(--pot-spacer) * 3.2)",
        "pot-spacer-3-600": "calc(var(--pot-spacer) * 3.6)",
        "pot-spacer-4": "calc(var(--pot-spacer) * 4)",
        "pot-spacer-4-800": "calc(var(--pot-spacer) * 4.8)",
        "pot-spacer-5-600": "calc(var(--pot-spacer) * 5.6)",
        "pot-spacer-6": "calc(var(--pot-spacer) * 6)",
        "pot-spacer-6-400": "calc(var(--pot-spacer) * 6.4)",
        "pot-spacer-7-200": "calc(var(--pot-spacer) * 7.2)",
        "pot-spacer-8": "calc(var(--pot-spacer) * 8)",
        "pot-spacer-10": "calc(var(--pot-spacer) * 10)",
        "pot-font-size-0": "var(--pot-spacer)",
        "pot-font-size-100": "var(--pot-spacer-1-200)",
        "pot-font-size-200": "var(--pot-spacer-1-400)",
        "pot-font-size-300": "var(--pot-spacer-1-600)",
        "pot-font-size-400": "var(--pot-spacer-1-800)",
        "pot-font-size-500": "var(--pot-spacer-2)",
        "pot-font-size-600": "var(--pot-spacer-2-400)",
        "pot-font-size-700": "var(--pot-spacer-3-200)",
        "pot-font-size-800": "var(--pot-spacer-4-800)",
        "pot-font-size-900": "var(--pot-spacer-6-400)",
        "pot-font-size-1000": "var(--pot-spacer-7-200)",
        "pot-transition": "0.4s ease",
        "pot-shadow-100": "0px 12px 32px 4px rgba(0, 0, 0, 0.04)",
        "pot-shadow-200": "0px 8px 20px rgba(0, 0, 0, 0.08)",
        "pot-shadow-300": "0px 16px 48px 16px rgba(0, 0, 0, 0.08)",
        "pot-shadow-400": "0px 0px 6px rgba(0, 0, 0, 0.12)",
        "pot-shadow-500": "0px 0px 12px rgba(0, 0, 0, 0.12)",
        "pot-shadow-600": "0px 12px 32px rgba(0, 0, 0, 0.12)",
        "pot-shadow-700": "0px 8px 16px -8px rgba(0, 0, 0, 0.16)",
        "pot-base-0": "#ffffff",
        "pot-base-100": "#f3f5f7",
        "pot-base-200": "#e7eaed",
        "pot-base-300": "#d3d9df",
        "pot-base-400": "#acaebb",
        "pot-base-500": "#5f6473",
        "pot-base-600": "#373a41",
        "pot-base-800": "#1c1e26",
        "pot-base-900": "#111",
        "pot-color-blue-100": "#c5dcf8",
        "pot-color-blue-200": "#5e9dec",
        "pot-color-blue-300": "#408be9",
        "pot-color-blue-400": "#1762c0",
        "pot-color-sea-100": "#abaaff",
        "pot-color-sea-200": "#8988ff",
        "pot-color-sea-300": "#7977ff",
        "pot-color-sea-400": "#5755ff",
        "pot-color-frog-100": "#B5D0C9",
        "pot-color-frog-200": "#21725e",
        "pot-color-frog-300": "#1B5F4E",
        "pot-color-frog-400": "#164c3f",
        "pot-color-peach-100": "#ffc7b5",
        "pot-color-peach-200": "#ffb197",
        "pot-color-peach-300": "#ffa589",
        "pot-color-peach-400": "#ff8f6b",
        "pot-color-mushroom-100": "#eebc8f",
        "pot-color-mushroom-200": "#efb37e",
        "pot-color-mushroom-300": "#f1a562",
        "pot-color-mushroom-400": "#f49b4d",
        "pot-color-pale-100": "#f4f3ff",
        "pot-color-pale-200": "#ecebff",
        "pot-color-pale-300": "#e1dfff",
        "pot-color-pale-400": "#d9d7ff",
        "pot-color-green-100": "#30f5a9",
        "pot-color-green-200": "#0acf83",
        "pot-color-green-300": "#09ba76",
        "pot-color-green-400": "#078755",
        "pot-color-grey-100": "var(--pot-base-300)",
        "pot-color-grey-200": "var(--pot-base-400)",
        "pot-color-grey-300": "var(--pot-base-500)",
        "pot-color-grey-400": "var(--pot-base-600)",
        "pot-color-orange-100": "#ffdcc1",
        "pot-color-orange-200": "#ff994d",
        "pot-color-orange-300": "#ff862c",
        "pot-color-orange-400": "#d85c00",
        "pot-color-red-100": "#f8bbc1",
        "pot-color-red-200": "#ed5564",
        "pot-color-red-300": "#ea3849",
        "pot-color-red-400": "#bd1425",
        "pot-color-yellow-100": "#fce7ae",
        "pot-color-yellow-200": "#f7c844",
        "pot-color-yellow-300": "#f6bf26",
        "pot-color-yellow-400": "#c49308",
        "pot-color-invalid-100": "var(--pot-color-red-100)",
        "pot-color-invalid-200": "var(--pot-color-red-200)",
        "pot-color-invalid-300": "var(--pot-color-red-300)",
        "pot-color-invalid-400": "var(--pot-color-red-400)",
        "pot-color-invalid": "var(--pot-color-invalid-300)",
        "pot-color-invalid-text": "var(--pot-base-0)"
    },
    "breakpoints": {
        "mobile": 0,
        "tablet": 768,
        "laptop": 1280,
        "desktop": 1440
    },
    "size": [
        "tiny",
        "small",
        "medium",
        "large"
    ],
    "color": {
        "pot": {
            "target": "var(--pot-color-mushroom-300)",
            "border": "var(--pot-color-mushroom-300)",
            "subcolor": "var(--pot-base-400)",
            "text": "var(--pot-base-0)",
            "hoverTarget": "var(--pot-color-mushroom-200)",
            "hoverBorder": "var(--pot-color-mushroom-200)",
            "hoverSubcolor": "var(--pot-base-500)",
            "hoverText": "var(--pot-base-0)",
            "activeTarget": "var(--pot-color-mushroom-400)",
            "activeBorder": "var(--pot-color-mushroom-400)",
            "activeSubcolor": "var(--pot-base-600)",
            "activeText": "var(--pot-base-0)",
            "disabledTarget": "var(--pot-base-400)",
            "disabledBorder": "var(--pot-base-400)",
            "disabledSubcolor": "var(--pot-base-500)",
            "disabledText": "var(--pot-base-0)",
            "invalidTarget": "var(--pot-color-invalid)",
            "invalidBorder": "var(--pot-color-invalid)",
            "invalidSubcolor": "var(--pot-base-400)",
            "invalidText": "var(--pot-color-invalid-text)"
        }
    },
    "radius": {
        "circle": "calc(var(--pot-spacer) * 100)",
        "large": "0.5em",
        "medium": "0.4em",
        "small": "0.3em",
        "tiny": "0.2em",
        "radius-8": 8,
        "radius-6": 6,
        "radius-4": 4,
        "radius-2": 2
    },
    "gap": {
        "large": "var(--pot-spacer-2)",
        "medium": "var(--pot-spacer-1-600)",
        "small": "var(--pot-spacer)",
        "tiny": "var(--pot-spacer-0-600)"
    },
    "components": {
        "button": {
            "color": {
                "base": {
                    "border": "var(--color-border)",
                    "background": "var(--color-target)",
                    "text": "var(--color-text)"
                },
                "hover": {
                    "background": "var(--color-hover-target)",
                    "border": "var(--color-hover-border)",
                    "text": "var(--color-hover-text)"
                },
                "pressed": {
                    "background": "var(--color-active-target)",
                    "border": "var(--color-active-target)",
                    "text": "var(--color-active-text)"
                },
                "disabled": {
                    "background": "var(--pot-base-400)",
                    "border": "var(--pot-base-400)",
                    "text": "var(--color-disabled-text)"
                }
            },
            "size": {
                "tiny": {
                    "height": "var(--pot-spacer-3-200)",
                    "text": "var(--pot-font-size-200)",
                    "padding": "var(--pot-spacer-1-200)",
                    "gap": "var(--pot-spacer-0-800)",
                    "icon": "var(--pot-spacer-1-400)",
                    "border": 1
                },
                "small": {
                    "height": "var(--pot-spacer-3-600)",
                    "text": "var(--pot-font-size-200)",
                    "padding": "var(--pot-spacer-1-600)",
                    "gap": "var(--pot-spacer-0-800)",
                    "icon": "var(--pot-spacer-1-600)",
                    "border": 1
                },
                "medium": {
                    "height": "var(--pot-spacer-4-800)",
                    "text": "var(--pot-font-size-200)",
                    "padding": "var(--pot-spacer-2)",
                    "gap": "var(--pot-spacer-1-200)",
                    "icon": "var(--pot-spacer-1-600)",
                    "border": 1
                },
                "large": {
                    "height": "var(--pot-spacer-5-600)",
                    "text": "var(--pot-font-size-300)",
                    "padding": "var(--pot-spacer-2-800)",
                    "gap": "var(--pot-spacer-1-200)",
                    "icon": "var(--pot-spacer-2)",
                    "border": 1
                }
            }
        }
    }
}